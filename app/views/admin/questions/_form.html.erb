<%= form_with model: [:admin, @test, @question], class: "form-inline", data: { turbo: false } do |f| %>
  <div class="mb-3">
    <%= f.label :body, class: 'form-label' %>
    <%= f.text_field :body, id: :question_body, class: "form-control" %>
  </div>

  <div class="mb-3">
    <%= f.label :image_url, class: 'form-label' %>
    <%= f.text_field :image_url, class: 'form-control mb-3' %>
  </div>

  <%= f.submit t('.submit'), class: "btn btn-primary" %>
<% end %>
